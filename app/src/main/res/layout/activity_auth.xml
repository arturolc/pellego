<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:background="?colorControlHighlight"
    android:name="com.gitlab.capstone.pellego.fragments.auth.AuthActivity"
    tools:context="com.gitlab.capstone.pellego.fragments.auth.AuthActivity">

    <FrameLayout
        android:id="@+id/auth_fragment_container"
        android:layout_width="match_parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="?colorControlHighlight"
        android:layout_height="match_parent"
        android:elevation="15dp"
        tools:ignore="MissingConstraints">

        <fragment
            android:id="@+id/fragment6"
            android:name="androidx.navigation.fragment.NavHostFragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="?colorControlHighlight"
            app:defaultNavHost="true"
            app:navGraph="@navigation/auth_nav"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"
            />
    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>